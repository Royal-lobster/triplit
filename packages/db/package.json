{
  "name": "@triplit/db",
  "version": "0.0.37",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "typings": "dist/index.d.ts",
  "types": "dist/index.d.ts",
  "source": "src/index.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "scripts": {
    "build": "yarn build:code && yarn build:types",
    "build:types": "yarn tsc --p tsconfig.build.json",
    "build:code": "yarn build:setup && parcel build . --no-cache && yarn build:cleanup",
    "build:setup": "mv tsconfig.json tsconfig.tmp.json && mv tsconfig.build.json tsconfig.json",
    "build:cleanup": "mv tsconfig.json tsconfig.build.json && mv tsconfig.tmp.json tsconfig.json",
    "lint": "tsc",
    "test": "vitest",
    "test-coverage": "vitest --coverage",
    "test-coverage-ui": "vitest --ui --coverage",
    "test-ci": "vitest run --coverage.enabled --coverage.reporter='text-summary'",
    "bench": "vitest bench",
    "coverage": "stryker run",
    "publish-pkg": "node ../../scripts/npm-check-version-and-publish.js"
  },
  "files": [
    "/dist"
  ],
  "devDependencies": {
    "@parcel/config-default": "^2.9.3",
    "@parcel/transformer-typescript-tsc": "^2.9.3",
    "@stryker-mutator/core": "^7.1.1",
    "@stryker-mutator/vitest-runner": "^7.1.1",
    "@vitest/coverage-c8": "^0.29.2",
    "@vitest/coverage-v8": "^0.33.0",
    "@vitest/ui": "^0.33.0",
    "parcel": "^2.9.3",
    "rxdb": "^14.14.2",
    "rxjs": "^7.8.1",
    "tinybench": "^2.5.0",
    "typescript": "^5.2.2",
    "vitest": "^0.32.2"
  },
  "dependencies": {
    "@sinclair/typebox": "^0.27.4",
    "ajv-formats": "^2.1.1",
    "idb": "^7.1.1",
    "nanoid": "^3.0.0",
    "pino": "^8.11.0",
    "sorted-btree": "^1.8.1",
    "tinybench": "^2.5.0",
    "tuple-database": "^2.2.0",
    "zod": "^3.20.6"
  }
}
